{
  "name": "@shexjs/parser",
  "version": "1.0.0-alpha.15",
  "description": "Shape Expressions Compact Syntax (ShExC) parser.",
  "author": {
    "name": "Eric Prud'hommeaux",
    "email": "eric@w3.org",
    "url": "http://www.w3.org/People/Eric/"
  },
  "keywords": [
    "shex",
    "shape expressions",
    "rdf",
    "query",
    "parser"
  ],
  "license": "MIT",
  "main": "./lib/shex-parser.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "@shexjs/util": "^1.0.0-alpha.15",
    "@ts-jison/parser": "^0.0.9"
  },
  "devDependencies": {
    "@ts-jison/parser-generator": "^0.0.9"
  },
  "scripts": {
    "parser": "ts-jison -n ShExJison -t javascript -p lr -o ./src/ShExJison.ts ./src/ShExJison.jison",
    "test": "cd .. && mocha -C -R dot",
    "build": "rimraf dist && npm run parser && tsc",
    "build:browser": "rimraf browser && tsc --outDir browser --module esnext",
    "browser": "browserify shex.js -s ShEx -o browser/shex-parser-browserify.js && uglifyjs browser/shex-parser-browserify.js --comments -mco browser/shex-parser-browserify.min.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shexjs/shex.js.git"
  },
  "bugs": {
    "url": "https://github.com/shexSpec/grammar/issues"
  }
}
